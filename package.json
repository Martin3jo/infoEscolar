{
  "name": "esc-jorge-newbery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build-css": "sass --load-path=public/scss public/scss/style.scss public/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "concurrently \"npm run start\" \"npm run watch-css\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "sass": "^1.75.0"
  }
}
