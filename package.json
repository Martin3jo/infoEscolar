{
  "name": "esc-jorge-newbery",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "build-css": "sass --load-path=public/scss public/scss/style.scss public/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "concurrently \"npm run start\" \"npm run watch-css\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.10.1",
    "nodemailer": "^6.9.14",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "sass": "^1.75.0"
  }
}
